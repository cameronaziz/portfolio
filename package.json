{
  "name": "react-static-example-apollo-redux",
  "version": "1.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "react-static start",
    "stage": "react-static build --staging",
    "build": "react-static build",
    "serve": "serve dist -p 3000",
    "jest": "jest --coverage --watch --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!tests/**/*.*",
    "jest-single": "jest --coverage --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!tests/**/*.*",
    "jest-debug": "jest --coverage -i --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!tests/**/*.*"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.0.0",
    "apollo-client": "^2.0.1",
    "apollo-link-http": "^1.1.0",
    "axios": "^0.16.2",
    "dotenv": "^6.0.0",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.5.0",
    "json-loader": "^0.5.7",
    "pouchdb": "^7.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.0.0",
    "react-apollo": "^2.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^4.3.11",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-static": "^5.9.8",
    "react-static-routes": "^1.0.0",
    "redux": "^3.7.2",
    "styled-components": "^3"
  },
  "devDependencies": {
    "babel-jest": "^23.6.0",
    "babel-plugin-styled-components": "^1.3.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-react-tools": "1.x.x",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "jest": "^23.6.0",
    "react-test-renderer": "^16.5.2",
    "serve": "^6.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "verbose": true,
    "collectCoverage": true,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/testsConfig/transformer.js",
      "\\.(css|less)$": "<rootDir>/tests/testsConfig/transformer.js"
    }
  }
}
